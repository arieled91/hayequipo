<?xml version='1.0' encoding='utf-8'?>
<widget id="com.arieled91.hayequipo" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Futboldesa</name>
    <description>
        A Futbol Match Planner
    </description>
    <author email="arieled91@gmail.com" href="https://github.com/arieled91/hayequipo">
        arieled91
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-hot-code-push-plugin" spec="^1.5.3" />
    <plugin name="cordova-hot-code-push-local-dev-addon" spec="^0.4.2" />
    <icon src="icon.png" />
    <engine name="android" spec="^7.0.0" />
</widget>
